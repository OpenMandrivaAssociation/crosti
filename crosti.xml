<?xml version="1.0" encoding="UTF-8"?>
<mime-info xmlns='http://www.freedesktop.org/standards/shared-mime-info'>
    <mime-type type="text/x-cst">
        <sub-class-of type="text/plain" />
        <comment>Crosti cross stitch</comment>
        <glob pattern="*.cst" />
        <magic priority="50">
            <match-case value="#COLOR TABLE" type="string" offset="0" />
        </magic>
    </mime-type>
</mime-info>
